<!DOCTYPE html>
<html lang="<%- blogData.lang %>">



<head>
  
  <%- include('head/setup-pre-variables.ejs') %>
  
  <%- include('head/load-backend-variables/variables.ejs') %>
  
  <%- include('head/head-common-part1.ejs') %>
  
  <%- include('head/meta-title-canonical-loader/meta-title-can.ejs') %>

  <%- include('head/css-loader/css.ejs') %>

  <%- include('head/schema-loader/schema.ejs') %>

  <%- include('head/code-element/code-highlight.ejs') %>

  <% if (blogData.has_equ === true) { %>
    <%- include('head/math-equation-render/math-equations.ejs') %>
  <% } %>
  

</head>






<body>

  <%- include('body/nav.ejs') %>



  <!-- Welcome Section -->


  <section id="tmWelcome" class="parallax-window" data-parallax="scroll" data-image-src="<%= welcome_section_fr.banner_img_path %>"
    alt="<%= welcome_section_fr.banner_img_alt %>">

    <%- include('body/welcome-section/part1.ejs') %>

    <div class="tm-bg-black-transparent tm-welcome-container">
      <div class="container-fluid">


        <div class="row h-100">

          <%- include('body/welcome-section/part2.ejs') %>

        </div>
      </div>
    </div>
  </section>


  <!-- End Welcome section -->











  <!-- Portfolio section -->
  <section id="tmPortfolio">
    <div class="container-fluid">
      <div class="row">
        <div id="blog-posting" class="col-12">

          <div>
            <div id="bar1">
              <span class="category-template"><%= blogData.category.category_name %></span>
              <div id="dates">
                <span id="date-published">
                  Published: <%= new Date(blogData.datetime_published).toLocaleDateString('en-US', {
                    year: 'numeric',
                    month: 'short',
                    day: 'numeric'
                  }) %>
                </span>
                <span id="date-edited">
                  Edited: <%= new Date(blogData.datetime_edited).toLocaleDateString('en-US', {
                    year: 'numeric',
                    month: 'short',
                    day: 'numeric'
                  }) %>
                </span>
              </div>
            </div>


            <div id="content-template" class="tm-extra-service-description"><%- blogData.html_content %></div>
          </div>

          
        </div>
      </div>
    </div>
  </section>


  <%- include('footer/footer.ejs') %>

  </body>


</html>