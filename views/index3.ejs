                <a target="_blank" class="animated-text blue mb-0" href="https://calendly.com/drywallkingston/30min">
                  <%= nav_fr.op2 %>
                </a>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="tm-bg-black-transparent tm-welcome-container">
        <div class="container-fluid">
          <div class="row h-100">
            <!-- Welcome Text -->
            <!-- White transparent bg -->
            <div class="col-md-7 tm-welcome-left-col">
              <div class="tm-welcome-left">
                <span class="tm-welcome-title">
                  <%= welcome_section_fr.op1 %>
                </span>
                <p class="pb-0" itemscope itemtype="https://schema.org/Organization">
                  <a rel="nofollow" href="/organization" itemprop="url">
                    <%= business_data_fr.street_address %>
                      <%= business_data_fr.address_city %>, <%= business_data_fr.address_province_state %>
                          <%= business_data_fr.postal_code %>
                            <%= business_data_fr.address_country %>
                  </a>
                  <!-- 1786 Bath Rd #18, Kingston, ON K7M 4Y2 -->
                </p>



                <div>
                  <!-- <a style="text-decoration: underline;" href="#/extra-link">Extra link</a> -->
                  <a style="text-decoration: underline;" href="/about">
                    <%= welcome_section_fr.op2 %>
                  </a>
                </div>



              </div>
            </div>

            <!-- Brown bg -->
            <div class="col-md-5">
              <a href="/organization">
                <div class="tm-welcome-right"
                  aria-label="Map showing the local drywall business location in Kingston, Ontario.">
                  <img src="/img/map1_op.avif"
                    alt="Map showing the local drywall business location in Kingston, Ontario." style="display:none;">
                </div>
              </a>

            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- End Welcome section -->






    <!-- Portfolio section -->
    <section id="tmPortfolio">
      <div class="container-fluid">
        <div class="row">





          <div class="col-12">




            <% main_service_data_fr.forEach((service, i)=> { %>



              <div class="tm-portfolio-item">
                <div class="tm-portfolio-name text-white tm-bg-green"
                  aria-label="<%= service.service_image_description %>">

                  <img src="<%= service.service_image_path %>"
                    alt="<%= service.service_image_description %>"
                    style="display:none;">

                </div>

                <style>
                  /* Select the first occurrence of the matched div */
                  section#tmPortfolio .tm-portfolio-item:nth-of-type(<%= i+1 %>) .tm-portfolio-name {
                    background-image: url('..<%= service.service_image_path %>');
                  }
                </style>


                <div class="tm-portfolio-description">
                  <h2 class="tm-text-black">
                    <%= service.service_name %>
                      <a class="tm-title-small" href="/service/<%= service.slug %>">
                        <%= portfolio_section_fr.op1 %>
                      </a>
                  </h2>
                  <p class="mb-0">
                  <h3>
                    <%= service.service_sub_title %>
                  </h3>
                  <div><%- service.service_description %></div>
                  </p>
                </div>
              </div>
            <% }); %>





              <%
              const more_service_img_alt = "A close-up view of a driveway with a combination of asphalt and interlocking stone pavers, showing a clean transition between the two materials and bordered by a stone curb."
              %>

                <div class="tm-portfolio-item">
                  <div class="tm-portfolio-name text-white tm-bg-blue"
                    aria-label="<%= more_service_img_alt %>">
                    <img src="/img/more_services1.webp"
                      alt="<%= more_service_img_alt %>" style="display:none;">
                  </div>


                  <style>
                    section#tmPortfolio .tm-portfolio-item:nth-of-type(4) .tm-portfolio-name {
                      background-image: url('../img/more_services1.webp');
                    }
                  </style>



                  <div class="tm-portfolio-description">
                    <h2 class="tm-text-black">
                      <%= portfolio_section_fr.op2 %>
                      <a class="tm-title-small" href="#">
                        <%= portfolio_section_fr.op1 %>
                      </a>
                    </h2>
                    
                    <p class="mb-0">
                    <div>
                      <ul>
                        <% extra_service_pages_fr.forEach(function(service) { %>
                          <li><a href="/service/<%= service.slug %>"><%= service.title %></a></li>
                        <% }); %>
                      </ul>


                    </div>
                    </p>
                  </div>
                </div>




                <% index_content_fr.forEach((content, i)=> { %>

                  <div class="tm-portfolio-item">
                    <div class="tm-portfolio-name text-white tm-bg-purple"
                      aria-label="<%= content.img_alt %>">
                      <img src="<%= content.img_path %>"
                        alt="<%= content.img_alt %>"
                        style="display:none;">
                    </div>


                    <style>
                      section#tmPortfolio .tm-portfolio-item:nth-of-type(<%= i+5 %>) .tm-portfolio-name {
                        background-image: url('..<%= content.img_path %>');
                      }
                    </style>




                    <div class="tm-portfolio-description">
                      <h2 class="tm-text-black">
                        <%= content.title %>
                        <!-- <a class="tm-title-small" href="/service/drywall-industry-overview">More details</a> -->
                      </h2>



                      <p class="mb-0">
                        <h3><%= content.sub_title %></h3>
                        <div><%= content.content %></div>
                      </p>
                    </div>
                  </div>

                <% }); %>










          </div>




        </div>
      </div>
    </section>
    <!-- End portfolio section -->





    <!-- Review section -->
    <div id="tmContact" class="parallax-window" data-parallax="scroll" data-image-src="/img/pavage_gatineau_b1.webp"
    alt="Two workers installing a large drywall sheet in a residential construction project in Kingston, Ontario.">
      <div class="container-fluid">

        <div class="row">
          <div class="col-12">

            <div class="tm-contact-items-container">




              <% review_data_fr.forEach(function(review) { %>
              <div class="tm-contact-item">
                <!-- Review Text -->
                <p>
                  <%= review.review_body %>
                </p>
                <!-- Reviewer Name -->
                <p class="mb-0">
                  <%= review.name %>
                </p>

                <!-- Star Rating -->
                <div class="star-rating">
                  <div class="stars-<%= Math.floor(review.rating_value) %>">
                    <% for(let i = 1; i <= 5; i++) { %>
                      <span class="star <%= i <= Math.floor(review.rating_value) ? 'filled' : (i === Math.ceil(review.rating_value) && review.rating_value % 1 !== 0 ? 'half-filled' : '') %>">â˜…</span>
                    <% } %>
                  </div>
                </div>

                <div class="svg-wrapper"></div>
                <div class="svg-wrapper2"></div>
              </div>
              <% }); %>


          </div>
        </div>
      </div>





        <div id="FAQ" class="row">
          <div class="col-12">
            <div class="tm-bg-darker-blue-transparent" itemscope itemtype="https://schema.org/FAQPage">
              <h2>FAQ</h2>
        
              <% faq_fr.forEach(function(faq) { %>
                <details itemprop="mainEntity" itemscope itemtype="https://schema.org/Question">
                  <summary itemprop="name"><%= faq.id %>. <%= faq.question %></summary>
                  <div itemprop="acceptedAnswer" itemscope itemtype="https://schema.org/Answer">
                    <span itemprop="text"><%= faq.answer %></span>
                  </div>
                </details>
              <% }); %>
        
            </div>
          </div>
        </div>
        


      </div>
    </div>









