    <!-- Welcome Section -->
    <section id="tmWelcome" class="parallax-window" data-parallax="scroll" data-image-src="/img/pavage_gatineau_b1.webp"
      alt="<%= welcome_section_fr.banner_img_alt %>">


      <div class="container-fluid tm-brand-container-outer">
        <div class="row">
          <div class="col-12">
            <!-- style="overflow: hidden;" -->
            <!-- Logo Area -->
            <!-- Black transparent bg -->
            <div class="ml-auto mr-0 tm-bg-black-transparent text-white tm-brand-container-inner">
              <div class="tm-brand-container text-center">
                <!-- <h1>Index2</h1> -->
                <span class="tm-brand-name">
                  <h1 class="tm-brand-name long-title">
                    <%= index_fr.title %>
                  </h1> ðŸ‡¨ðŸ‡¦
                </span>
                <p class="tm-brand-description">
