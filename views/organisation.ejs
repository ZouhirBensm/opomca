<!DOCTYPE html>
<html lang="en">

<head>

  <%- include('head/setup-pre-variables.ejs') %>
  
  <%- include('head/load-backend-variables/variables.ejs') %>
  
  <%- include('head/head-common-part1.ejs') %>
  
  <%- include('head/meta-title-canonical-loader/meta-title-can.ejs') %>
  
  <%- include('head/css-loader/css.ejs') %>
  
  <%- include('head/schema-loader/schema.ejs') %>

</head>








<body>

  <%- include('body/nav.ejs') %>







    <!-- Welcome Section -->

    <section id="tmWelcome" class="parallax-window" data-parallax="scroll" data-image-src="<%= welcome_section_fr.banner_img_path %>"
      alt="<%= welcome_section_fr.banner_img_alt %>">

      <%- include('body/welcome-section/part1.ejs') %>

      <div class="tm-bg-black-transparent tm-welcome-container">
        <div class="container-fluid">


          <div class="row h-100">

            <%- include('body/welcome-section/part2.ejs') %>

          </div>
        </div>
      </div>
    </section>
    <!-- End Welcome section -->





  <!-- Portfolio section -->

  <section id="tmPortfolio">
    <div class="container-fluid">


      <!-- <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2858.487007906932!2d-76.57952612376648!3d44.23821897108038!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x84e3f24c39e20253%3A0xe1daa8a5ca255ec3!2sDrywall%20Kingston%20Earnanswers!5e0!3m2!1sen!2sca!4v1718386506505!5m2!1sen!2sca" 
      style="border:0; width: 100%; height: 450px;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe> -->




      <div id="org" class="row">
        <div class="col-12">

          <header>
            <h1>Welcome to <%= business_data_fr.business_name %></h1>
            <p>We are a <%= business_data_fr.service_type %> based in <%= business_data_fr.address_city %>, <%= business_data_fr.address_province_state %>, Canada.</p>
          </header>
      
          <section>
            <h2>About Us</h2>
            <p><%= business_data_fr.business_description %></p>
          </section>
      
          <section>
            <h2>Contact Information</h2>
            <ul>
                <li><strong>Website:</strong> <a href="<%= business_data_fr.website_main_url %>"><%= business_data_fr.website_main_url %></a></li>
                <li><strong>Email:</strong> <a href="mailto:<%= business_data_fr.email %>"><%= business_data_fr.email %></a></li>
                <li><strong>Telephone:</strong> <%= business_data_fr.telephone %></li>
                <li><strong>Alternative Telephone:</strong> <%= business_data_fr.telephone2 %></li>
                <li><strong>Address:</strong> <%= business_data_fr.street_address %>, <%= business_data_fr.address_city %>, <%= business_data_fr.address_province_state %>, <%= business_data_fr.postal_code %>, <%= business_data_fr.address_country %></li>
            </ul>
          </section>
      
          <section>
            <h2>Scheduling</h2>
            <p>If you'd like to schedule a meeting, feel free to use our Calendly link: <a href="<%= business_data_fr.calendly_link %>">Schedule a meeting</a></p>
          </section>
      
          <section>
            <h2>Location</h2>
            <p>Our office is located at the coordinates: Latitude: <%= business_data_fr.latitude %>, Longitude: <%= business_data_fr.longitude %></p>
          </section>
      
          <section>
            <p>&copy; <%= new Date().getFullYear() %> <%= business_data_fr.business_name %></p>
          </section>

        </div>
      </div>


    </div>
  </section>
  <!-- End portfolio section -->





  <%- include('footer/footer.ejs') %>

  </body>

</html>